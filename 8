#include <stdio.h>

int main() {
    int a, b;

    // Get input from the user
    printf("Enter first number: ");
    scanf("%d", &a);
    printf("Enter second number: ");
    scanf("%d", &b);

    // Compare the two numbers
    if (a > b) {
        printf("A is greater than B\n");
    } else if (b > a) {
        printf("B is greater than A\n");
    } else {
        printf("A is equal to B\n");
    }

    return 0;
}
