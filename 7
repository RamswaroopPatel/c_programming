#include <stdio.h>

int main() {
    int a1, b1;
//ramswaroop
    // Get input from the user
    printf("Enter first number: ");
    scanf("%d", &a1);
    printf("Enter second number: ");
    scanf("%d", &b1);

    // Swap the values without a temporary variable
    a1 = a1 + b1; // a now holds the sum of a and b
    b1 = a1 - b1; // b is now the original a
    a1 = a1 - b1; // a is now the original b

    // Print the results
    printf("First number is %d\n", a1); 
    printf("Second number is %d\n", b1); 

    return 0; 
}
